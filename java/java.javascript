const Btn-open-popup =
document.querySelector(Btn-open-popup)
const Popup =
document.querySelector(Popup)
const Btn-close-popup =
document.querySelector(Btn-close-popup)
 
Btn-open-popup.addEventListener("click"()=>{
Popup.showPopup();
})
Btn-close-popup.addEventListener("click"()=>{
    Popup.close();
    })